<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="mainFrame" class="View">
        <property name="title">
            <value>Address Book</value>
        </property>
        <constructor-arg ref = "book"/>
        <constructor-arg ref = "controller"/>
    </bean>
    <bean id="book" class="AddressBook"/>
    <bean id="controller" class="Controller"/>

</beans>